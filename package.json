{
  "name": "toshstabletaskstorments",
  "version": "0.0.1",
  "description": "CS 340 Final Project - Tosh is a barkeep who fields questions from a lot of adventurers, this enchanted table helps him keep the facts straight.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_QuestsCard": "handlebars views/partials/QuestsCard.handlebars -f public/QuestsCardTemplate.js",
    "build_MonstersCard": "handlebars views/partials/MonstersCard.handlebars -f public/MonstersCardTemplate.js",
    "build_AbilitiesCard": "handlebars views/partials/AbilitiesCard.handlebars -f public/AbilitiesCardTemplate.js",
    "build_LootItemsCard": "handlebars views/partials/LootItemsCard.handlebars -f public/LootItemsCardTemplate.js",
    "build_QuestsTitle": "handlebars views/partials/QuestsTitle.handlebars -f public/QuestsTitleTemplate.js",
    "prestart": "npm run build_QuestsCard && npm run build_MonstersCard && npm run build_AbilitiesCard && npm run build_LootItemsCard && npm run build_QuestsTitle",
    "offline": "node server.js -offline",
    "local_online": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trentonyo/ToshsTableTasksTorments.git"
  },
  "author": "Trenton Young, Josh Williams",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trentonyo/ToshsTableTasksTorments/issues"
  },
  "homepage": "https://github.com/trentonyo/ToshsTableTasksTorments#readme",
  "dependencies": {
    "express": "^4.18.2",
    "express-handlebars": "^6.0.7",
    "forever": "^4.0.3",
    "handlebars": "^4.7.7",
    "mysql": "^2.18.1"
  }
}
